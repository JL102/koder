{
  "name": "koder-react",
  "version": "1.6.0",
  "homepage": "./",
  "private": true,
  "description": "QR/bar code scanner for the Browser",
  "author": "Pavel Maslov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/preset-react": "^7.9.0",
    "@pathcheck/dcc-sdk": "^0.0.23",
    "http-server": "^0.13.0",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "upnqr": "1.1.0"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "scripts": {
    "start": "HTTPS=true PORT=8080 HOST=0.0.0.0 react-scripts start",
    "build": "react-scripts build && npm run build-sw && npm run clean-cra-sw",
    "build-sw": "node ./src/sw-build.js",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "prod": "http-server ./build/ -p 8082 -c-1",
    "vanilla-js": "http-server ./docs/ -p 8081 -c-1",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pre-publish-node": "cp LICENSE dist && cp README.md dist && cp public/wasm/zbar* dist"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maslick/koder.git"
  },
  "bugs": {
    "url": "https://github.com/maslick/koder/issues"
  },
  "keywords": [
    "qrcode",
    "barcode",
    "webassembly",
    "wasm",
    "emscripten",
    "zbar",
    "qr",
    "ean13",
    "ean8",
    "i25",
    "upca",
    "upce"
  ],
  "dependencies": {
    "typescript": "^4.5.5"
  }
}
